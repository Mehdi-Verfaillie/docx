{
  "name": "docx",
  "license": "MIT",
  "description": "A doc just in time to access the right doc when you need it the most",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Education",
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:ShortcutMenuBar.openFilesList",
    "onCommand:ShortcutMenuBar.openFile",
    "onCommand:ShortcutMenuBar.userButton01",
    "onStartupFinished",
    "onCommand:extension.showDropdown"
  ],
  "main": "./out/extension.ts",
  "contributes": {
    "configuration":[
      {
        "title": "Get your documentaion",
        "properties": {
          "extension.openDropdown": {
            "title": "show opened files list",
            "type": "boolean",
            "default": true,
            "description": "show icon for Opened files list"
          },
          "ShortcutMenuBar.openFile": {
            "title": "Open file",
            "type": "boolean",
            "default": false,
            "description": "show icon for 'Open File'"
          },
          "ShortcutMenuBar.userButton01Command": {
            "title": "command",
            "type": "string",
            "default": null,
            "markdownDescription": "command executed with 'user action 1'; _icon is shown if set_"
          }
        }
      }
    ], 
    "commands": [
      {
        "command": "ShortcutMenuBar.openFilesList",
        "title": "Show opened files list",
        "category": "ShortcutMenuBar",
        "icon": {
          "light": "images/book-dark.svg",
          "dark": "images/book-light.svg"
        }
      },
      {
        "command": "ShortcutMenuBar.openFile",
        "title": "Open file",
        "category": "ShortcutMenuBar",
        "icon": {
          "light": "images/book-dark.svg",
          "dark": "images/book-light.svg"
        }
      },
      {
        "command": "ShortcutMenuBar.userButton01",
        "title": "Affiche laaa",
        "category": "ShortcutMenuBar",
        "icon": {
          "light": "images/book-dark.svg",
          "dark": "images/book-light.svg"
        }
      },
      {
        "category": "extension",
        "title": "Affiche moi ca laa",
        "icon": {
          "light": "images/book-dark.svg",
          "dark": "images/book-light.svg"
        }
      }
    ],
    "keybindings": [
      {
        "command": "ShortcutMenuBar.userButton01",
        "key": "ctrl+alt+1",
        "mac": "shift+cmd+1"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "!isInDiffEditor && !markdownPreviewFocus && config.ShortcutMenuBar.openFilesList",
          "command": "ShortcutMenuBar.openFilesList",
          "group": "navigation@7"
        },
        {
          "when": "config.ShortcutMenuBar.openFile",
          "command": "ShortcutMenuBar.openFile",
          "group": "navigation@17"
        },
        {
          "when": "config.ShortcutMenuBar.userButton01Command",
          "command": "ShortcutMenuBar.userButton01",
          "group": "navigation@30"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == markdown",
          "command": "markdown.showPreview",
          "alt": "markdown.showPreviewToSide",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test:lint": "eslint . --cache --cache-location .cache/eslint",
    "test:types": "tsc --noEmit",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write ./src"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/sinon": "^10.0.16",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "chai": "^4.3.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "sinon": "^15.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {}
}
