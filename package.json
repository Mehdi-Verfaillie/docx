{
  "name": "docx",
  "displayName": "docx",
  "description": "",
  "version": "0.0.1",
  "publisher":"docx-mt5",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-plugin.openFile",
        "title": "My plugin : Open File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test:lint": "eslint . --cache --cache-location .cache/eslint",
    "test:types": "tsc --noEmit",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write ./src"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
